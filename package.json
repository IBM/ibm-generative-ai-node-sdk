{
  "name": "@ibm-generative-ai/node-sdk",
  "version": "1.0.1",
  "description": "Node.js SDK for IBM Generative AI API",
  "keywords": [
    "ai",
    "generative",
    "genai",
    "ibm"
  ],
  "homepage": "https://workbench.res.ibm.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/ibm-generative-ai-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/IBM/ibm-generative-ai-node-sdk/issues"
  },
  "packageManager": "yarn@3.5.0",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./langchain": {
      "types": "./dist/langchain.d.ts",
      "import": "./dist/langchain.js",
      "require": "./dist/langchain.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepack": "yarn build && pinst --disable",
    "postinstall": "husky install",
    "postpack": "pinst --enable",
    "example:generate": "ts-node -r dotenv-flow/config examples/generate.ts"
  },
  "peerDependencies": {
    "langchain": ">=0.0.75"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.5",
    "@types/node-fetch": "^2.6.4",
    "@types/promise-retry": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/coverage-c8": "^0.31.2",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "jest-extended": "^3.2.4",
    "langchain": "^0.0.75",
    "lint-staged": "^13.2.0",
    "lodash": "^4.17.21",
    "msw": "^1.2.1",
    "pinst": "^3.0.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "axios-cache-interceptor": "^1.0.1",
    "cross-fetch": "^3.1.6",
    "promise-retry": "^2.0.1",
    "yaml": "^2.3.0",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "lint-staged": {
    "*.{cjs,js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
